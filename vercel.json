{
  "version": 2,
  "builds": [{
    "src": "./api/index.js",
    "use": "@vercel/node"
  },
    {
      "src": "./post/index-post.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [{
    "src": "/api/auth(.*)",
    "dest": "/api/index.js"
  },
    {
      "src": "/api/users(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/posts(.*)",
      "dest": "/post/index.js"
    }
  ]
}